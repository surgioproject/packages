{
  "name": "@surgio/gateway",
  "description": "Surgio API Gateway 面板",
  "private": false,
  "license": "MIT",
  "version": "2.0.0-alpha.3",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/surgioproject/packages/tree/master/packages/gateway",
  "author": "Roy Li <me@royli.dev>",
  "bugs": {
    "url": "https://github.com/surgioproject/surgio/issues"
  },
  "homepage": "https://surgio.js.org",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.build.json",
    "start": "nest start",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "lint": "eslint . --ext .ts",
    "test": "run-s test:ci",
    "test:unit": "jest",
    "test:e2e": "jest --config __tests__/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register jest --runInBand",
    "test:ci": "run-s test:unit test:e2e"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.1",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/serve-static": "^3.0.1",
    "@surgio/gateway-frontend": "workspace:*",
    "bluebird": "^3.7.2",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "nunjucks": "^3.2.4",
    "passport": "^0.6.0",
    "passport-cookie": "^1.0.9",
    "passport-http-bearer": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "semver": "^7.5.1",
    "serverless-http": "^3.2.0",
    "source-map-support": "^0.5.21",
    "youch": "^3.2.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.1",
    "@types/bluebird": "^3.5.38",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.194",
    "@types/nunjucks": "^3.2.2",
    "@types/passport": "^1.0.12",
    "@types/semver": "^7.5.0",
    "@types/supertest": "^2.0.12",
    "sinon": "^15.0.4",
    "supertest": "^6.3.3",
    "tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "surgio": "3.0.0-alpha.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "../__tests__/setup-tests.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "gitHead": "da96fe077b24097d0a721392bd32ec4a88ac2ee0"
}
